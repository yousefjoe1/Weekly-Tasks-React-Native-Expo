{
  "expo": {
    "name": "weekly-tasks",
    "slug": "weekly-tasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main-app.jpg",
    "scheme": "weeklytasks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
      "RECEIVE_BOOT_COMPLETED",
      "WAKE_LOCK"
    ],

      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
    "RECEIVE_BOOT_COMPLETED", 
    "WAKE_LOCK",
    "POST_NOTIFICATIONS" 
  ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.youssefdev.weeklytasks"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/main-app.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-background-fetch",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/main-app.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ba0ae848-f8f3-4768-9232-53209fbd273d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ba0ae848-f8f3-4768-9232-53209fbd273d"
    }
  }
}
